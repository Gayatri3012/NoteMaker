<section class="pagination mt-6 ">
    <% if (currentPage !=1 && previousPage !==1) { %>
        <a href="?page=1" class="p-3 m-4" >1</a>
        <% } %>
            <% if(hasPreviousPage) { %>
                <a href="?page=<%= previousPage %>" class="p-3 m-4" >
                    <%= previousPage %>
                </a>
                <% } %>
                    <a href="?page=<%= currentPage %>" class="active p-3 m-4">
                        <%= currentPage %>
                    </a>

                    <% if(hasNextPage) { %>
                        <a href="?page=<%= nextPage %>" class="p-3 m-4">
                            <%= nextPage %>
                        </a>
                        <% } %>
                            <% if(lastPage !==currentPage && nextPage !==lastPage) { %>
                                <a href="?page=<%= lastPage %>" class="p-3 m-4" >
                                    <%= lastPage %>
                                </a>
                                <% } %>
</section>